<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head') %>
  <body>
    <div>
      <form action="/categories" method="post">
        <label for="category">Enter New Category</label>
        <input type="text" id="category" name="category" />
        <button>Submit</button>
      </form>
      <% if (errors) { %> <% errors.forEach(error =>{ %>
      <h3><%= error.msg %></h3>
      <% }) %> <% } %>
    </div>
  </body>
</html>
